<div class="form-container">
  <%= simple_form_for(@feeling) do |f| %>
    <%= f.label :content %>
    <%= f.text_field :content, class: "form-control" %>

    <%= f.label :price %>
    <%= f.text_field :price, class: "form-control" %>

    <%= f.submit "submit", class: "btn btn-primary" %>
  <% end %>
</div>
